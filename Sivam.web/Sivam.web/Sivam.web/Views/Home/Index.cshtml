<style>
    .loader {
        border: 10px solid #f3f3f3;
        border-radius: 50%;
        border-top: 10px solid #3498db;
        width: 50px;
        height: 50px;
        -webkit-animation: spin 2s linear infinite; /* Safari */
        animation: spin 2s linear infinite;
        top: 50%;
        left: 50%;
        position: absolute;
    }

    /* Safari */
    @@-webkit-keyframes spin {
        0% {
            -webkit-transform: rotate(0deg);
        }

        100% {
            -webkit-transform: rotate(360deg);
        }
    }

    @@keyframes spin {
        0% {
            transform: rotate(0deg);
        }

        100% {
            transform: rotate(360deg);
        }
    }

    #btnResendOTP {
        background: none;
        border: none;
        color: navy;
    }

        #btnResendOTP:disabled,
        #btnResendOTP[disabled] {
            background-color: #fff;
            color: #ccc;
        }

        #btnResendOTP:hover {
            background-color: #fff;
        }
</style>
<script>
    $(document).ajaxStart(function () {
        $("#divLoader").css("display", "block");
    });

    $(document).ajaxComplete(function () {
        $("#divLoader").css("display", "none");
    });
    function setTimerforResend() {
        var count = 30;
        var counter = setInterval(timer, 1000); //1000 will  run it every 1 second
        function timer() {
            $('#btnResendOTP').attr('disabled', 'disabled')
            $('#spnTimer').text(count)
            count = count - 1;
            if (count <= 0) {
                $('#btnResendOTP').removeAttr('disabled')
                clearInterval(counter);
                $('#spnTimer').text('')
                //counter ended, do something here
                return;
            }

            //Do code for showing the number of seconds here
        }
    }
    $(document).ready(function () {
        var array = ["15-2-2021", "17-2-2021"]
        $(function () {
            $("#my_datepicker").datepicker({
                dateFormat: 'dd/mm/yy',
                minDate: 0,
                maxDate: +3,
                beforeShowDay: function (date) {
                    var day = date.getDay();
                    var string = jQuery.datepicker.formatDate('d-m-yy', date);
                    var isDisabled = ($.inArray(string, array) != -1);
                    return [day != 0 && !isDisabled];
                }
            });
        });
    })
    $("#btnSubmit").click(function (e) {
        var form = $("#bookingform");
        //form.validate();
        if (form.valid()) {
            event.preventDefault();
            var obj = {}
            obj.RecId = $('#RecId').val();
            obj.Name = $('#Name').val();
            obj.Email = $('#Email').val();
            obj.Mobile = $('#Mobile').val();
            obj.BookingDate = $('#my_datepicker').val();
            obj.TimeSlotId = $('#TimeSlotId').val();
            obj.DeptId = $('#DeptId').val();
            obj.SecureKey = $('#SecureKey').val();
            $.ajax({
                url: "/Home/GetDetails",
                type: "POST",
                contentType: "application/json;charset=UTF-8",
                dataType: "json",
                data: JSON.stringify(obj),
                success: function (data) {
                    $('#spnSuccessMsg').text(data.message)
                    if (data.success) {
                        $('#hidrecid').val(data.recid);
                        $('#hidsecurekey').val(data.securekey);
                        $('#divSuccess').show();
                        $('#spnSuccessMsg').text(data.message)
                        $('#divOTPDetails').show();
                        setTimerforResend();
                        $('#divBooking').hide();
                    }
                    else {
                        $('#divBooking').show();
                        $('#divOTPDetails').hide();
                        $('#divError1').show();
                        $('#spnErrorMsg1').text(data.message)
                    }
                },
            });
        }

    });

    $('#btnValidate').click(function () {
        if ($('#txtOTP').val() == '' || $('#txtOTP').val() == null) {
            $('#spnOTPError').show();
            return;
        }
        $('#spnOTPError').hide();
        $.ajax({
            url: "/Home/ValidateDetails",
            type: "GET",
            contentType: "application/json;charset=UTF-8",
            dataType: "json",
            data: { OTP: $('#txtOTP').val(), RecId: $('#hidrecid').val(), secureKey: $('#hidsecurekey').val() },
            success: function (data) {
                debugger
                if (data.success && data.code == 200 || data.code == '200') {
                    $('#divConfirmation').show();
                    $('#divBooking,#divOTPDetails').hide();
                    setTimeout(function () { location.href='/'; }, 5000);
                }
                else if (data.success && data.code == 100 || data.code == '100') {
                  
                    $('#divError').show();
                    $('#spnErrorMsg').text(data.message)
                }
                else {
                    $('#divError').show();
                    $('#spnErrorMsg').text('something went wrong');
                }

            },
        });
    })

    $('#btnResendOTP').click(function () {
        setTimerforResend();
        $.ajax({
            url: "/Home/ReWriteDetails",
            type: "GET",
            contentType: "application/json;charset=UTF-8",
            dataType: "json",
            data: { RecId: $('#hidrecid').val(), secureKey: $('#hidsecurekey').val() },
            success: function (data) {
                if (data.success && data.code == 200 || data.code == '200') {
                    $('#divSuccess').show();
                    $('#spnSuccessMsg').text(data.message)
                }
                else if (data.success && data.code == 100 || data.code == '100') {
                    $('#divError').show();
                    $('#spnErrorMsg').text(data.message)
                }
                else {
                    $('#divError').show();
                    $('#spnErrorMsg').text('something went wrong');
                }

            },
        });
    })
</script>